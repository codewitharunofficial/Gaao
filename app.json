{
  "expo": {
    "name": "gaao",
    "slug": "gaao",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Gaao-Logo.png",
    "scheme": "gaao",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Gaao-Logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codewitharun.gaao.ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.codewitharun.gaao"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      [
        "expo-av", {
          "microphonePermission": "Allow Gaao to access your microphone."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a474f8a6-a1e2-4e4a-a978-67ff2d82879a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a474f8a6-a1e2-4e4a-a978-67ff2d82879a"
    }
  }
}
