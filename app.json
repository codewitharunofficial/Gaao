{
  "expo": {
    "name": "Gaao",
    "slug": "gaao",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/Gaao-Icon.png",
    "scheme": "gaao",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Gaao-Icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codewitharun.gaao.ios"
    },
    "assetBundlePatterns": [
      "assets.images/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "MANAGE_EXTERNAL_STORAGE"
      ],
      "package": "com.codewitharun.gaao"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      [
        "expo-av", {
          "microphonePermission": "Allow Gaao to access your microphone."
        }
      ],
      [
        "expo-media-library",
        {
          "photoPermission": "Allow Gaao to access yoour photos",
          "savePhotoPermission": "Allow Gaao to save photos",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a474f8a6-a1e2-4e4a-a978-67ff2d82879a"
      },
      "googleClientId": "632816505768-19ut7in277tncklho1fonidsa6uve3mq.apps.googleusercontent.com"
    },
    "runtimeVersion": "1.0.3",
    "updates": {
      "url": "https://u.expo.dev/a474f8a6-a1e2-4e4a-a978-67ff2d82879a"
    }
  }
}
